<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="QuestionGUI">
  <grid id="27dc6" binding="questionPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="1280" height="720"/>
    </constraints>
    <properties>
      <background color="-1"/>
      <maximumSize width="1280" height="720"/>
      <minimumSize width="1280" height="720"/>
      <opaque value="true"/>
      <preferredSize width="1280" height="720"/>
    </properties>
    <border type="none"/>
    <children>
      <grid id="4f958" binding="progressBarPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties>
          <background color="-1"/>
        </properties>
        <border type="none"/>
        <children>
          <component id="77f6a" class="javax.swing.JProgressBar" binding="questionsProgressBar">
            <constraints border-constraint="North"/>
            <properties>
              <background color="-16777216"/>
              <font name="Berlin Sans FB Demi" size="30"/>
              <foreground color="-13312"/>
              <maximum value="15"/>
              <maximumSize width="1280" height="30"/>
              <minimumSize width="1280" height="30"/>
              <preferredSize width="1280" height="30"/>
              <string value="£100"/>
              <stringPainted value="true"/>
              <value value="5"/>
            </properties>
          </component>
          <grid id="7011c" binding="contentsPanel" layout-manager="GridBagLayout">
            <constraints border-constraint="Center"/>
            <properties>
              <background color="-1"/>
            </properties>
            <border type="none"/>
            <children/>
          </grid>
        </children>
      </grid>
      <grid id="1cb3a" binding="statsPanel" layout-manager="GridBagLayout">
        <constraints border-constraint="North"/>
        <properties>
          <background color="-1"/>
          <preferredSize width="1280" height="40"/>
        </properties>
        <border type="none"/>
        <children>
          <component id="b04ce" class="javax.swing.JLabel" binding="nameLabel">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <background color="-1"/>
              <font name="Berlin Sans FB Demi" size="25"/>
              <horizontalAlignment value="0"/>
              <horizontalTextPosition value="0"/>
              <preferredSize width="640" height="40"/>
              <text value="Player 1"/>
            </properties>
          </component>
          <component id="74f47" class="javax.swing.JLabel" binding="moneyLabel">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
              <gridbag weightx="0.0" weighty="0.0"/>
            </constraints>
            <properties>
              <background color="-1"/>
              <font name="Berlin Sans FB Demi" size="25"/>
              <horizontalAlignment value="0"/>
              <horizontalTextPosition value="0"/>
              <preferredSize width="640" height="40"/>
              <text value="£1000000"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
